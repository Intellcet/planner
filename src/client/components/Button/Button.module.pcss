.container {
  display: flex;
  justify-content: center;
  margin-right: 7px;
  border-radius: 10px;
  padding: 19px 5px;
  width: 155px;
  background: #d9d9d9;
  transition: 0.2s linear;
  cursor: pointer;
  &:hover {
    background-color: #d9d9d97c;
  }
}
.title {
  font-family: 'Inter-Regular', sans-serif;
  font-size: 16px;
  line-height: 19px;
}
